{
  "name": "portfolio-fotografico",
  "version": "1.0.0",
  "description": "Un moderno portfolio fotografico con mappa interattiva",
  "main": "index.js",
  "scripts": {
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && npm install",
    "setup:frontend": "cd frontend && npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:e2e": "npx playwright test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && eslint src/",
    "lint:frontend": "cd frontend && eslint src/",
    "clean": "npm run clean:backend && npm run clean:frontend",
    "clean:backend": "cd backend && rm -rf node_modules",
    "clean:frontend": "cd frontend && rm -rf node_modules"
  },
  "keywords": [
    "photography",
    "portfolio",
    "react",
    "nodejs",
    "leaflet",
    "map"
  ],
  "author": "Kevin",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuousername/portfolio-fotografico.git"
  },
  "bugs": {
    "url": "https://github.com/tuousername/portfolio-fotografico/issues"
  },
  "homepage": "https://github.com/tuousername/portfolio-fotografico#readme",
  "dependencies": {
    "react-grid-layout": "^2.2.2",
    "react-moveable": "^0.56.0",
    "three-slippy-map-globe": "^1.0.3"
  }
}
